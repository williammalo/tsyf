---
autoprefix: true
---
vendors = official

retina="(-webkit-min-device-pixel-ratio: 1.25),(min-resolution: 120dpi)"

@font-face {
	font-family: 'social';
	src:url('fonts/social.eot');
}
@font-face {
	font-family: 'social';
	src: url("data:application/x-font-woff;charset=utf-8;base64,") format('woff'),
		 url("data:application/x-font-ttf;charset=utf-8;base64,") format('truetype');
	font-weight: normal;
	font-style: normal;
}

brand=#c00
brand2=#ccc


/*
div:first-child:nth-last-child(4),
div:first-child:nth-last-child(4) ~ div
	width: (100%/4)
*/
*
	box-sizing:     border-box
	text-rendering: optimizeLegibility
	position:       relative

html,input
	font-family:'Open Sans',sans-serif
	color:#333
html
	font-size:20px
	line-height:0
	@media (min-width:2300px)
		font-size:42px
	@media (max-width:2300px)
		font-size:40px
	@media (max-width:2200px)
		font-size:38px
	@media (max-width:2100px)
		font-size:36px
	@media (max-width:2000px)
		font-size:34px
	@media (max-width:1900px)
		font-size:32px
	@media (max-width:1800px)
		font-size:30px
	@media (max-width:1700px)
		font-size:28px
	@media (max-width:1600px)
		font-size:26px
	@media (max-width:1500px)
		font-size:24px
	@media (max-width:1400px)
		font-size:22px
	@media (max-width:1300px)
		font-size:20px
	@media (max-width:880px)
		font-size:18px
	@media (max-width:810px)
		font-size:16px
	@media (max-width:685px)
		font-size:14px
	@media (max-width:560px)
		font-size:18px
	@media (max-width:460px)
		font-size:16px
body
	margin:0

a, input, button
	-ms-touch-action: none !important


p
	margin:0
	width:100%
	display:inline-block
	margin-bottom:1.4em
p:last-child
	margin-bottom:0
*:not(:last-child) p
	margin-bottom:1.4em
img
	max-width: 100%
	border: none
	height: auto

h1,h2,h3,h4,h5,h6
	font-family:'Open Sans Condensed',sans-serif
	text-transform:uppercase
	margin:0
	margin-bottom:1.4rem

a
	color:brand
	display:inline-block
	text-decoration: none
a:hover
	text-decoration: underline
a.inline
	display:inline
i
	font-family: 'social'
	font-style:normal
	text-decoration:none

table
	max-width:100%
	overflow:hidden

small
	font-size:.62em
	@media (max-width:685px)
		font-size:.8em

input,textarea
	width:100%
	display:inline-block
	font-size:1em
	line-height:1.2em
	padding:.4em
	padding-top:.3em
	padding-bottom:.1em
	border-radius:.3em
	border:solid .1em brand2
	outline:none
	margin:.1em
label.fixedwidth+input
	width:calc(100% - 6.2em)
input:focus,[type=checkbox]:focus+label:before,[type=file]:focus+label:after,textarea:focus
	border-color:brand

textarea
	width:100%

label
	display:inline-block
	-webkit-transform:translateZ(1px) //bugfix
.inline
	box-sizing:content-box
	margin:.1em
.fixedwidth
	width:6em
	text-align:right
	padding-right:.5em
td .fixedwidth
	width:5.7em

[type=submit],[type=file]+label:after
	padding:.4em
	padding-top:.2em
	padding-bottom:.2em
	color:#fff

[type=submit]
	background:brand
	border-color:brand
	&:focus,&:hover
		background:#fff
		color:brand

[type=checkbox],[type=file]
	width:1px
	position:absolute
	opacity:0

.ie8 [type=checkbox]
	width:auto
	position:relative
	opacity:1

[type=checkbox]+label:before, [type=file]+label:after, ,#i-autre~article [for="i-autre"]:before
	height:1em
	line-height:1
	text-align:center
	@extends input

[type=checkbox]+label:before,#i-autre~article [for="i-autre"]:before
	content:"✗"
	padding:0
	color:lighten(brand2,50%)
	width:1em
[type=file]+label:after
	background:brand2
	border-color:brand2
	cursor:pointer
	width:8em
[type=file]+label:hover:after,[type=file]:focus+label:after
	background:#fff
	color:brand2
[lang=fr] [type=file]+label:after
	content:"Choisir Fichier"
[lang=en] [type=file]+label:after
	content:"Choose File"

[type=checkbox]:checked+label:before,#i-autre:checked~article [for="i-autre"]:before
	content:"✓"
	color:#0c0

.ie8 [type=checkbox]+label:before
	display:none

.conditional
	display:none
.c:checked~.conditional
	display:block !important

.reverse-conditional
	display:block
.c:checked~.reverse-conditional
	display:none !important


::selection
	background:rgba(brand,.2)
	color:darken(brand,50%)

header
	overflow:hidden
	//box-shadow:inset 0 0 3em rgba(#000,.8)
	background:url(head2.jpg)
	padding-bottom:36%
	background-size:cover
	@media (max-width:750px)
		background-image:url("1500/head2.jpg")
	@media (max-width:500px)
		background-image:url("1000/head2.jpg")
	@media (max-width:350px)
		background-image:url("700/head2.jpg")

.contact header img,.rfq header img,.credit header img
	display:none

.home header
	background-image:url(head3.jpg)
	padding-bottom:29%
	@media (max-width:750px)
		background-image:url("1500/head3.jpg")
	@media (max-width:500px)
		background-image:url("1000/head3.jpg")
	@media (max-width:350px)
		background-image:url("700/head3.jpg")
.home header img
	display:none
.home header h1
	right:0
	left:none
	text-align:right
.about header
	background-image:url(head4.jpg)
	padding-bottom:35%
	@media (max-width:750px)
		background-image:url("1500/head4.jpg")
	@media (max-width:500px)
		background-image:url("1000/head4.jpg")
	@media (max-width:350px)
		background-image:url("700/head4.jpg")
.about header img
	top: 4.4rem;
.hiring header
	background-image:url(head5.jpg)
	padding-bottom:33%
	@media (max-width:750px)
		background-image:url("1500/head5.jpg")
	@media (max-width:500px)
		background-image:url("1000/head5.jpg")
	@media (max-width:350px)
		background-image:url("700/head5.jpg")

.documents header
	background-image:url(head5.jpg)
	padding-bottom:32%
	@media (max-width:750px)
		background-image:url("1500/head5.jpg")
	@media (max-width:500px)
		background-image:url("1000/head5.jpg")
	@media (max-width:350px)
		background-image:url("700/head5.jpg")

body header.img2
	background-image:url(head1-alt.jpg)
	padding-bottom:16.8% !important
	box-shadow:inset 0 0 3em rgba(#000,.4)
	@media (max-width:750px)
		background-image:url("1500/head1-alt.jpg")
	@media (max-width:500px)
		background-image:url("1000/head1-alt.jpg")
	@media (max-width:350px)
		background-image:url("700/head1-alt.jpg")

@keyframes kenburns
	from
		transform:scale3D(1.2,1.2,1)
		opacity:0
	to
		transform:scale3D(1,1,1)
		opacity:1

@keyframes fadein
	from
		opacity:0
	to
		transform:scale3D(1,1,1)

header img
	width:43%
	position:absolute
	top:48px//ie8
	top:3rem
	left:-2%
	@media (max-width:1020px)
		width:300px
	@media (max-width:800px)
		left:auto
		top:48px//ie8
		top:3rem
	@media (max-width:690px)
		top:16px//ie8
		top:1rem
		right:16px//ie8
		right:1rem
	@media (max-width:580px)
		width:250px
	@media (max-width:520px)
		display:none
.hiring header img
	display:none !important


h1
	padding:48px 96px 48px 96px//ie8
	padding:3rem 6rem .6rem 6rem
	font-size:5.4em
	color:white
	position:absolute
	left:0
	bottom:0
	text-shadow:0 0 .3em #000
	white-space:nowrap
	@media (max-width:560px)
		font-size:4em
		padding-bottom:24px//ie8
		padding-bottom:1.5rem
.credit h1,.rfq h1
	font-size:4em
	padding-bottom:0rem
	@media (max-width:560px)
		font-size:1.8em
nav
	background:#000
	line-height:1em
	position:fixed
	top:0
	right:0
	z-index:2 //some items on the page are z-index 1
	-webkit-transform:translateZ(1px)
	@media (max-width:560px)
		font-size:0.8em
	& a,& .dropdown
		color:brand2
		white-space:nowrap
		text-decoration:none
		display:inline-block
		padding:.5em 1em
		background:#000
		transform:translateZ(1px)
		transition:all .3s
		cursor:pointer
		&:hover
			background:brand
			color:#fff
			transition:all .3s
		& .logo:hover
			color:transparent

nav,.dropdown-container
	border-bottom:solid .3em brand2

.dropdown:after
	content:"\25BC"
	display:inline-block
	transform: scaleY(0.6) rotate(180deg) translateY(.1em)
	transition: all .3s

.dropdown.dropdown-closed:after
	transform: scaleY(0.6) rotate(0deg) translateY(.1em)
	transition: all .3s

.dropdown-container
	position:absolute
	left:0
	top:100%
	width:100%
	@media (max-width:800px)
		top:auto
		bottom:100%
		border-bottom:none

.dropdown-container a
	display:block

.dropdown .dropdown-container
	display:block
	height: 8.3em;
	transition: height .3s
	overflow:hidden

.dropdown-closed .dropdown-container
	//display:none
	height:0
	transition: height .3s

.logo
	color:transparent
	width:5em
	img
		height:1em
		position:absolute
		top:.5em
		left:.5em
		z-index:1

.services nav [href=servicesfr]
.services nav [href=servicesen]
.about nav [href=aboutfr]
.about nav [href=abouten]
.contact nav [href=contactfr]
.contact nav [href=contacten]
.hiring nav [href=embauchefr]
.hiring nav [href=embaucheen]
.intermodal nav [href=intermodalfr]
.intermodal nav [href=intermodalen]
	background:brand
	color:#fff

.home nav [href=fr]
.home nav [href=en]
	background:brand
	color:transparent

.services nav:hover [href=servicesfr]:not(:hover)
.services nav:hover [href=servicesen]:not(:hover)
.about nav:hover [href=aboutfr]:not(:hover)
.about nav:hover [href=abouten]:not(:hover)
.contact nav:hover [href=contactfr]:not(:hover)
.contact nav:hover [href=contacten]:not(:hover)
.hiring nav:hover [href=embauchefr]:not(:hover)
.hiring nav:hover [href=embaucheen]:not(:hover)
	background:#000
	color:brand2

.home nav:hover [href=fr]:not(:hover)
.home nav:hover [href=en]:not(:hover)
	background:#000
	color:transparent

html body .lang
	background:#000
	color:brand2

nav>a:first-of-type:after,nav>a:first-of-type:before
	content:""
	display:block
	position:absolute
	top:0
	left:-2em
	border:solid 0 transparent
	border-bottom:solid 2em transparent
	width:0
	height:0
	border-right:solid 2em #000
	transition:all .3s
	box-shadow:.25em 0 0 #000

nav>a:first-of-type:before
	left:-2.5em
	border-right:solid 2.3em brand2
	border-bottom:solid 2.3em transparent
	box-shadow:.25em 0 0 brand2

@media (max-width:1104px)
	nav
		width:100%
		text-align:center
		padding-right:0 !important
	nav a:before
		display:none !important
	.logo
		display:none !important


@media (max-width:800px)
	nav
		top:auto
		bottom:0
	footer
		padding-bottom:5em !important

section,.section
	background:#fff
	//-webkit-transform:translateZ(1px) //safari fix
article
	white-space: pre-wrap
	img,figure
		width:33%
		box-shadow:0 0 5px rgba(#000,.2)
		margin-bottom:.7em
		margin-top:.7em
	.left
		float:left
		margin-right:1em
	.right
		float:right
		margin-left:1em
	.noshadow
		box-shadow:none

.col1,.col2,.col,footer
	line-height:1.4em

.col1,.col2,.col
	display:inline-block
	vertical-align:top
.col1,.col2
	width:50%
.col
	column-count:2

@media (max-width:560px)
	.col
		column-count:1
	.col1,.col2
		width:100%

.cat
	width:250px
	position:absolute
	top:-107px
	right:10px
	//-webkit-transform:translateZ(1px) //safari fix
	@media (max-width:560px)
		width:150px
		top:-65px

footer,html
	background:darken(brand,7%)

footer
	border-top:solid 1em brand
	color:#100
	padding:3em
	text-align:center
	a
		color:#fff
		text-decoration:none
//		display:inline-block
//		transition:text-shadow .3s
//		text-shadow: 0 0 .3em rgba(#000,.3)
//		&:hover
//			text-shadow: 0 0 .3em rgba(#fff,.6)


[lang=fr] .en,[lang=en] .fr
	display:none !important

//whitespace

nav
	padding-right:96px//ie8
	padding-right:6rem
.col
	column-gap:6rem
header img, h1
	padding-left:96px//ie8
	padding-left:6rem
article
	padding:0 3em
.col1,.col2,.col
	padding:3em 3em

@media (max-width:1160px)
	nav
		padding-right:48px//ie8
		padding-right:3rem
	.col
		column-gap:3rem
	header img, h1
		padding-left:48px//ie8
		padding-left:3rem
	article
		padding:0 1.5em
	.col1,.col2,.col
		padding:3em 1.5em

.col1.nopad,.col2.nopad,.col.nopad
	padding-bottom:0

article .col.nocol
	column-count:1

.ws-popover
	-webkit-transform:translateZ(1px) //safari fix

.documents ul
	padding:0
	list-style:none
	width:100%
.documents li
	text-align:center
	display:inline-block
	vertical-align:top
	width:33.33%
	margin-bottom:2.8em
	padding: 0 1em
.documents li img
	display:block
	margin:0 auto
	margin-bottom:1.4em
	width:60%
@media (max-width:710px)
	.documents li
		width:50%
@media (max-width:390px)
	.documents li
		width:100%



.contact section>img
.contact .section>img
	width:100%
	margin-top:-28%
	z-index:-1

.documents section>img
.documents .section>img
	width:100%
	margin-top:-15%
.documents article
	z-index:1

@media (max-width:560px)
	.contact section>img
	.contact .section>img
		margin-top:0


.contact section
.contact .section
	background:#fff url(foot3.jpg) center bottom no-repeat
	background-size:contain

#i-autre:checked~article .other-hidden{
	display:none
}


